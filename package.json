{
  "name": "stock-keeper",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon --watch '**/*.ts' --watch '**/*.json' --watch '**/*.graphql' --exec 'ts-node' src/app.ts",
    "compile": "tsc --noEmit",
    "test": "./node_modules/.bin/jest --watchAll",
    "test-ci": "./node_modules/.bin/jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globalSetup": "./jest.config.js",
    "collectCoverage": true
  },
  "license": "MIT",
  "dependencies": {
    "@types/helmet": "^0.0.38",
    "@types/jest": "^23.1.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "graphql-yoga": "^1.14.10",
    "helmet": "^3.12.1",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.2",
    "typescript": "^2.9.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
