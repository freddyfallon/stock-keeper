{
  "name": "stock-keeper",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon --watch '**/*.ts' --watch '**/*.json' --watch '**/*.graphql' --exec 'ts-node' src/app.ts",
    "compile": "tsc --noEmit",
    "test": "jest --watchAll",
    "test-ci": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globalSetup": "./jest.config.js",
    "collectCoverage": true
  },
  "license": "MIT",
  "dependencies": {
    "@types/helmet": "^0.0.38",
    "babel-preset-env": "^1.7.0",
    "graphql-yoga": "^1.14.10",
    "helmet": "^3.12.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/nock": "^9.1.3",
    "@types/request-promise-native": "^1.0.15",
    "@types/ws": "^5.1.2",
    "babel-cli": "^6.26.0",
    "jest": "^22.4.3",
    "nock": "^9.3.3",
    "nodemon": "^1.17.5",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.2",
    "typescript": "^2.9.2",
    "util": "^0.11.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
